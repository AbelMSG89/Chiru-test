---
import Layout from "@/layouts/Layout.astro";
import { getStaticLanguagePaths } from "@/i18n/utils";
import { TemplateRegister } from "@/components/templates/TemplateRegister";

export async function getStaticPaths() {
  return getStaticLanguagePaths();
}

const imageUrl = "https://i.pinimg.com/736x/14/47/c3/1447c3dde903d7ac5681ca2e774a3411.jpg"; // Cambia por la ruta de tu imagen
const formProps = {};
---

<Layout title="Ejemplo TemplateRegister">
  <TemplateRegister
    client:load
    imageUrl={imageUrl}
    imageAlt="Register"
    formProps={formProps}
  />
</Layout>